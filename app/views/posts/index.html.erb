<h2>作品一覧</h2>
<table>
<thead>
<tr>
<th>ニックネーム</th>
<th>作品</th>

<th>コメント</th>
</thead>

<tbody>
<% @posts.each do |post| %>
<tr>
<td><%= post.name %></td>
<td><%= post.image%></td>
<td><%= post.coment%></td>
<td><%= post.created_at.to_s(:dateime_jp)%></td>
<td><%= post.updateed_at.to_s(:datetime_jp)%></td>
<%end%>

</tbody>
</table>



